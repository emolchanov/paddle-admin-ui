{
  "name": "paddle-app",
  "productName": "paddle-app",
  "version": "1.0.1",
  "description": "Paddle Admin UI",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "next:dev": "next dev",
    "next:build": "next build",
    "next:start": "next start",
    "next:lint": "next lint",
    "next:static": "node scripts/copy-static.js",
    "app:open": "open ./out/native-app-darwin-arm64/native-app.app",
    "prepare": "npm run app:clean",
    "prestart": "npm run node:dll-download",
    "prebuild": "npm run app:clean && npm run node:dll-download",
    "node:dll-download": "node scripts/dll-download.js",
    "app:clean": "rimraf out && rimraf .webpack && rimraf node_modules/.cache && rimraf bins && rimraf .next",
    "build": "npm run next:build && npm run next:static && npm run make"
  },
  "keywords": [],
  "author": {
    "name": "Eugene Molchanov",
    "email": "eugene.a.molchanov@gmail.com"
  },
  "license": "MIT",
